{
  "name": "steady-submit",
  "version": "0.0.1",
  "description": "Auto-submission scripts for Steady",
  "keywords": [
    "steady",
    "scripts",
    "markdown"
  ],
  "license": "MIT",
  "author": "Shane Martin",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md}\"",
    "lint": "eslint --cache --fix .",
    "prepare": "husky install",
    "test": "vitest"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,ts,md}": [
      "eslint --fix",
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "dependencies": {
    "hast-util-to-html": "^9.0.3",
    "mdast-util-from-markdown": "^2.0.1",
    "mdast-util-to-hast": "^13.2.0",
    "puppeteer": "^23.6.0",
    "tslib": "^2.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/eslint__js": "^8.42.3",
    "@types/mdast": "^4.0.4",
    "@types/node": "^20.16.13",
    "eslint": "^9.13.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "mdast-util-gfm": "^3.0.0",
    "micromark-extension-gfm": "^3.0.0",
    "prettier": "^3.3.3",
    "sort-package-json": "^2.10.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0"
  },
  "volta": {
    "node": "20.18.0"
  }
}
